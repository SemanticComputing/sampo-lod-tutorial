        {
            "id": "additionalInfo",
            "valueType": "string",
            "makeLink": false,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false,
            "minWidth": 250,
            "onlyOnInstancePage": true,
            "renderAsHTML": true
        },
        {
            "id": "producedPerformances",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": false,
            "numberedList": false,
            "minWidth": 250,
